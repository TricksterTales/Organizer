package com.trickstertales.main;

public class Main {

	private MainGui gui;

	public static void main(String args[]) {
		Main main = new Main();
		main.start();
	}

	public Main() {
		gui = new MainGui(this);
	}
	
	public void start() {
		gui.setVisible(true);
	}
	
	public void execute(String cmd) {
		gui.println(cmd);
	}

}
